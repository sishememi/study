package chap3;

public class InfraredRaySenser {
	private String name;
	private boolean objectFounded;
	public InfraredRaySenser(String name) {
		this.name=name;
	}
	public void foundObject() {
		this.objectFounded=true;
	}
	public String getName() {
		return name;
	}
	public boolean isObjectFounded() {
		return objectFounded;
	}
	@Override
	public String toString() {
		return "InfraredRaySenser [name=" + name + ", objectFounded=" + objectFounded + "]";
	}
	
}
