package chap5;
/*
 * 반복문 예제
 * */
public class LoopEx1 {
	public static void main(String[] args) {
		System.out.print(1);
		System.out.print(2);
		System.out.print(3);
		System.out.print(4);
		System.out.println(5);
		System.out.println("for 구문으로 1~5까지 출력하기");
		//i = 5
		for(int i=1;i<=5;i++) {
			System.out.print(i);//12345
		}
		System.out.println("While 구문으로 1~5까지 출력하기");
		int i=1;
		while(i<=5) {
			System.out.println(i);
			i++;
		}
	}

}
