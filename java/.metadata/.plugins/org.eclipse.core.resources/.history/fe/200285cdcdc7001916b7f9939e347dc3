package chap5;
/*
 * 반복문 예제
 * */
public class LoopEx1 {
	public static void main(String[] args) {
		System.out.print(1);
		System.out.print(2);
		System.out.print(3);
		System.out.print(4);
		System.out.print(5);
		System.out.println(6);
		System.out.println("for 구문으로 1~5까지 출력하기");
		for(int i=1;i<=5,i++) {
			System.out.println(i);
		}
		
	}

}
